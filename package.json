{
  "name": "redux-live-class-recipe-app",
  "version": "1.0.0",
  "description": "An MVP of a React+Redux-based Recipe management app for a live class.",
  "scripts": {
    "api": "NODE_ENV=development nodemon --exec babel-node -- api.js",
    "start": "NODE_ENV=development node server.js",
    "test": "tape -r babel-register 'src/**/*spec.js' | faucet",
    "cover": "nyc -x '**/*spec.js' -r text -r html -r lcov npm test",
    "lint": "eslint src"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0",
      "react"
    ],
    "plugins": [
      "react-hot-loader/babel"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/joshdmiller/redux-live-class-recipe-app.git"
  },
  "author": "Josh David Miller <josh@joshdavidmiller.com> (http://joshdavidmiller.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/joshdmiller/redux-live-class-recipe-app/issues"
  },
  "homepage": "https://github.com/joshdmiller/redux-live-class-recipe-app",
  "devDependencies": {
    "babel-core": "^6.0.20",
    "babel-eslint": "^4.1.3",
    "babel-loader": "^6.0.1",
    "babel-preset-es2015": "^6.0.15",
    "babel-preset-react": "^6.0.15",
    "babel-preset-stage-0": "^6.0.15",
    "enzyme": "^2.3.0",
    "eslint": "^1.10.3",
    "eslint-plugin-react": "^3.6.2",
    "faucet": "0.0.1",
    "fetch-mock": "^5.0.2",
    "nyc": "^7.0.0",
    "react-addons-test-utils": "^15.2.0",
    "react-hot-loader": "^3.0.0-beta.1",
    "tape": "^4.6.0",
    "webpack": "^2.1.0-beta.14",
    "webpack-dev-server": "^2.1.0-beta.0"
  },
  "dependencies": {
    "babel-cli": "^6.11.4",
    "body-parser": "^1.15.2",
    "cors": "^2.7.1",
    "express": "^5.0.0-alpha.2",
    "material-ui": "^0.15.2",
    "nodemon": "^1.9.2",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.5.2",
    "react-router-redux": "^4.0.5",
    "react-tap-event-plugin": "^1.0.0",
    "redux": "^3.5.2",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.1.0",
    "shortid": "^2.2.6",
    "whatwg-fetch": "^1.0.0"
  }
}
